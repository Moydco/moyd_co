<div class='noise-wrapper'>
  <div class='header-main header-tp' id='anchorHome'>
    <div class='container'>
      <nav class='navbar navbar-default' role='navigation'>
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class='navbar-header'>
          <button class='navbar-toggle' data-target='.navbar-ex1-collapse' data-toggle='collapse' type='button'>
            <span class='sr-only'>Toggle navigation</span>
            <span class='icon-bar'></span>
            <span class='icon-bar'></span>
            <span class='icon-bar'></span>
          </button>
          <a class='current navbar-brand' href='/'>
            <%= image_tag 'logo.png', size: 45 %>
          </a>
        </div>
        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class='collapse navbar-collapse navbar-ex1-collapse'>
          <% if user_signed_in? %>
            <%= link_to destroy_user_session_path, method: :delete, class: 'btn btn-default navbar-btn navbar-right' do %>
              Sign Out <i class='icon-signout'></i>
            <% end %>
          <% else %>
            <%= link_to new_user_session_path, class: 'btn btn-default navbar-btn navbar-right' do %>
              Sign In <i class='icon-signin'></i>
            <% end %>
          <% end %>
          <ul class='nav navbar-nav navbar-right'>
            <% if user_signed_in? %>
              <% if controller_name == 'dashboard' %>
                <li class='current active'>
                  <%= link_to 'My dashboard', dashboard_index_path, class: 'current' %>
                </li>
              <% else %>
                <li>
                  <%= link_to 'My dashboard', dashboard_index_path %>
                </li>
              <% end %>
            <% end %>

            <% if action_name == 'support' %>
              <li class='current active'>
                <%= link_to 'App Development', support_statics_path, class: 'current' %>
              </li>
            <% else %>
              <li>
                <%= link_to 'App Development', support_statics_path %>
              </li>
            <% end %>

            <% if action_name == 'infrastructure' %>
              <li class='current active'>
                <%= link_to 'Infrastructure', infrastructure_statics_path, class: 'current' %>
              </li>
            <% else %>
              <li>
                <%= link_to 'Infrastructure', infrastructure_statics_path %>
              </li>
            <% end %>

            <% if action_name == 'dns' %>
              <li class='current active'>
                <%= link_to 'DNS App', dns_statics_path, class: 'current' %>
              </li>
            <% else %>
              <li>
                <%= link_to 'DNS App', dns_statics_path %>
              </li>
            <% end %>

            <% if action_name == 'team' %>
              <li class='current active'>
                <%= link_to 'Team', team_statics_path, class: 'current' %>
              </li>
            <% else %>
              <li>
                <%= link_to 'Team', team_statics_path %>
              </li>
            <% end %>
            <li>
              <a href='/blog'>Blog</a>
            </li>
            <% if action_name == 'contact_us' %>
              <li class='current active'>
                <%= link_to 'Contact Us', contact_us_statics_path, class: 'contact_us' %>
              </li>
            <% else %>
              <li>
                <%= link_to 'Contact Us', contact_us_statics_path %>
              </li>
            <% end %>
          </ul>
        </div>
      </nav>
    </div>
  </div>

  <div class='separator-shadow-bottom'>
    <%= image_tag 'shadow-separator-wide-bottom.png' %>
  </div>
</div>
